<main>
  <div class="container px-5 px-sm-0">
    <div class="row">
      <div class="col-md-3">
        <%= render 'users/info', user: @user %>
        <%= render 'books/list', book: @book %>
      </div>
      <div class="col-md-8 offset-md-1">
        <h2>Users</h2>
        <table class="table table-hover table-inverse">
          <thed>
            <tr>
              <th>image</th>
              <th>name</th>
              <th colspan="3"></th>
              </tr>
          </thed>
          <tbody>
            <% @users.each do |user| %>
            <tr>
              <td><%= image_tag user.get_profile_image("40x40") %></td>
              <td><%= user.name %></td>
              <td><%=link_to "Show", user_path(user.id), class: "user_52299" %></td>
            </tr>
            <tr></tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</main>
